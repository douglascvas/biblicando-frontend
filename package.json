{
  "name": "biblicando-frontend",
  "version": "0.0.1",
  "license": "ISC",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "localhost",
    "devPort": 8000,
    "title": "Biblicando"
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^3.4.34",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.4",
    "@types/nunjucks": "0.0.32",
    "@types/react": "^15.0.4",
    "@types/react-dom": "^0.14.21",
    "@types/react-helmet": "0.0.28",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-router": "^3.0.3",
    "@types/sinon": "^1.16.35",
    "@types/source-map-support": "^0.2.28",
    "@types/webpack": "^2.2.5",
    "@types/webpack-dev-server": "^1.12.6",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "axios": "^0.15.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-rewire": "^1.0.0",
    "babel-runtime": "^6.22.0",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "consolidate": "^0.14.5",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.0",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-browserify": "^0.5.1",
    "gulp-bump": "2.5.1",
    "gulp-changed": "1.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-express": "0.3.5",
    "gulp-if": "^2.0.2",
    "gulp-nodemon": "2.2.1",
    "gulp-notify": "2.2.0",
    "gulp-nunjucks": "^3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-protractor": "3.0.0",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.4.0",
    "gulp-typescript": "3.1.4",
    "gulp-uglify": "^2.0.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "gulp-wrap-commonjs": "^0.1.14",
    "history": "^4.5.1",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
