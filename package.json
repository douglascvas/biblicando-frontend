    "react-router": "^3.0.2",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.1.6",
    "source-map-support": "^0.4.10",
    "sourcemapped-stacktrace": "^1.1.5",
    "style-loader": "^0.13.1",
